   (repeat_ms 80))
(spew
   #include spew.dta)
#ifdef HX_XBOX
#define NON_GAME_TIMERS
(world hud track file_read tasks)
#else
#define NON_GAME_TIMERS
(world hud track file_read end_draw tasks)
#endif
(timer NON_GAME_TIMERS game cpu gs)
(rnd
   (title
      "Guitar Hero 2")
   #ifdef HX_EE
   (show_timers FALSE)
   #endif
   (overlays
      (time
         (lines 1)
         (showing FALSE)
         (color 0 0 0 0.7))
      (cheats
         (lines 1)
         (showing FALSE)
         (color 0 0 0 0.7))
      (camera
         (lines 4)
         (showing FALSE)
         (color 0 0 0 0.5))
      #ifdef HX_EE
      (light_preset
         (lines 4)
         (showing FALSE)
         (color 0 0 0 0.5))
      #endif
      (score
         (lines 15)
         (showing FALSE))
      #ifdef HX_XBOX
      (char_status
         (lines 1)
         (showing FALSE))
      (char_history
         (lines 3)
         (showing FALSE))
      #else
      (guitar
         (lines 1)
         (showing FALSE))
      (guitarist0_status
         (lines 1)
         (showing FALSE))
      (guitarist0_history
         (lines 3)
         (showing FALSE))
      #endif
      (player0_freestyle
         (lines 1)
         (showing FALSE))
      (score_debug
         (lines 1)
         (showing FALSE))
      (lights
         (lines 1)
         (showing FALSE))
      (camtest
         (lines 1)
         (showing FALSE))
      (transition_test
         (lines 1)
         (showing FALSE)))
   #ifdef HX_XBOX
   (gamma 0.8)
   #endif
   (timer_script
      {do
         ($tot 0)
         {foreach
            $t
            (NON_GAME_TIMERS)
            {set
               $tot
               {'+'
                  $tot
                  {timer_ms $t}}}}
         {set_timer_ms
            game
            {-
               {timer_ms cpu}
               $tot}}}))
